@model IList<EntityModel.EventModel>

@{ 
    ViewData["Titil"] = "Events";
}
<div class="container">

    @if (Model.Count == 0)
            {
                <div>
                    <h2>No events to show!</h2>
                </div>
            }
            else
            {
                 <h2>All events</h2>
                 <div class="row">
                     @foreach (var events in Model)
                     {
                     <dl class="row">
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.Title)
                         </dt
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.Title)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.Date)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.Date)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.Location)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.Location)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.StartTime)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.StartTime)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.Type)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.Type)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.Duration)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.Duration)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.Description)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.Description)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.OtherDetails)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.OtherDetails)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.InvitedBy)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.InvitedBy)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.UserId)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.UserId)
                         </dd>
                         <dt class="col-sm-2">
                             @Html.DisplayNameFor(model => events.RegistereEmail)
                         </dt>
                         <dd class="col-sm-10">
                             @Html.DisplayFor(model => events.RegistereEmail)
                         </dd>
                     </dl>
                 
                         }
                 </div>
            }

</div>